{
  "name": "fast-date",
  "version": "3.0.2",
  "description": "Fast UTC Date Timestamps",
  "main": "index.js",
  "scripts": {
    "test": "tap -R spec --no-cov 'test/*.js'",
    "cov": "tap --100 'test/*.js'",
    "cov-badge": "tap --coverage-report=lcov && codecov -t 417ad444-d6e4-4fa6-b778-27d9b5317f02",
    "bench": "node bench",
    "bench-http": "node bench/http",
    "lint": "standard",
    "ci": "npm run cov && npm run cov-badge"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "keywords": [
    "date",
    "fast",
    "performant",
    "performance",
    "time"
  ],
  "author": "David Mark Clements <huperekchuno@googlemail.com>",
  "license": "MIT",
  "devDependencies": {
    "codecov": "^1.0.1",
    "fastbench": "^1.0.1",
    "optional-dev-dependency": "^2.0.0",
    "pre-commit": "^1.2.2",
    "standard": "^9.0.0",
    "tap": "^10.2.2"
  },
  "optionalDevDependencies": {
    "autocannon": [
      "^2.4.0",
      "bench-http"
    ]
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidmarkclements/fast-date.git"
  },
  "bugs": {
    "url": "https://github.com/davidmarkclements/fast-date/issues"
  },
  "homepage": "https://github.com/davidmarkclements/fast-date#readme"
}
